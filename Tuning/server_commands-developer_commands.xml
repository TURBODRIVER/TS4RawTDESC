<?xml version="1.0" encoding="iso-8859-1"?>

<TuningRoot>
    <Module name="server_commands.developer_commands">
        <Class name="CheatWoohooTuning">
            <TunableList name="CHEAT_WOOHOO_BITS" class="TunableList" display="Cheat Woohoo Bits" group="General">
                <Tunable type="relbit" class="TunableReference" allow_none="False" group="General" />
            </TunableList>

            <Tunable type="buff" name="CHEAT_WOOHOO_BUFF" class="TunableReference" display="Cheat Woohoo Buff" allow_none="False" group="General" />

            <Tunable type="statistic" name="CHEAT_WOOHOO_COMMODITY" class="TunableReference" display="Cheat Woohoo Commodity" allow_none="False" group="General" restrict="Commodity" />

            <Tunable type="statistic" name="CHEAT_WOOHOO_SOCIALCONTEXT" class="TunableReference" display="Cheat Woohoo Socialcontext" allow_none="False" group="General" restrict="RelationshipTrack" />

            <Tunable type="statistic" name="CHEAT_WOOHOO_TRACK" class="TunableReference" display="Cheat Woohoo Track" allow_none="False" group="General" restrict="RelationshipTrack" />
        </Class>

        <Class name="CheatWeddingTuning">
            <TunableList name="CHEAT_ENGAGED_RELATIONSHIP_BITS" class="TunableList" display="Cheat Engaged Relationship Bits" description="Relationship bits added to the sims that are being cheated into engaged status." group="General">
                <Tunable type="relbit" class="TunableReference" allow_none="False" group="General" restrict="RelationshipBit" />
            </TunableList>
        </Class>
    </Module>
</TuningRoot>